{
  "expo": {
    "name": "발등에 불",
    "slug": "hot-foot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/info/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/info/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.suyoooi.hotfoot",
      "buildNumber": "1.0.5",
      "sound": true,
      "permissions": ["NOTIFICATIONS"],
      "infoPlist": {
        "NSUserNotificationUsageDescription": "푸시 알림을 허용하면 새로운 알림을 받을 수 있습니다.",
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleDevelopmentRegion": "ko",
        "CFBundleLocalizations": ["ko"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/info/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "sound": true,
      "permissions": ["NOTIFICATIONS"]
    },
    "notification": {
      "iosDisplayInForeground": true,
      "android": {
        "sound": true,
        "icon": "./assets/icon.png"
      },
      "ios": {
        "sound": true,
        "icon": "./assets/icon.png"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "92cf28cf-62ad-4456-aacd-d33c6d64b0dd"
      }
    },
    "owner": "suyoooi",
    "plugins": [
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sound/BB-06_finish.mp3",
            "./assets/sound/BB-06_start.mp3",
            "./assets/sound/BB-06_start_10.mp3",
            "./assets/sound/BB-06_start_20.mp3",
            "./assets/sound/BB-06_next.mp3",
            "./assets/sound/BB-go_out5.mp3",
            "./assets/sound/BB-go_out10.mp3"
          ]
        }
      ]
    ]
  }
}
